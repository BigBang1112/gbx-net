{
  "metadata": [
    {
      "src": [
        {
          "src": "../../Src",
          "files": [
            "**/bin/Release/**.dll"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net8.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "GBX.NET 2",
      "_appTitle": "GBX.NET 2 Documentation",
      "_appFooter": "Â© 2024 BigBang1112",
      "_enableSearch": true,
      "pdf": false
    }
  }
}